type Dolar {
  dolar_date: String
  price_blue: Float
  price_official: Float
  price_mep: Float
}

extend type Query {
  getDolar(date: String!): Dolar
  getDolarInDateRange(startDate: String!, endDate: String!): [Dolar]
  getHistoricalDolar: [Dolar]
}

extend type Mutation {
  createDolar: Dolar
}
